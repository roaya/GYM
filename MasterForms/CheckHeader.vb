Public Class CheckHeader

    Dim Gcls As GeneralSp.NewMasterForms
    Dim B_EndLoad As Boolean = False
    Dim BSourceCheckHeader As New BindingSource
    '-------------------------------
    'Set The Data Table Name
    Dim TName As String = "Check_Header"
    '-------------------------------


    Private Sub Categories_FormClosed(ByVal sender As Object, ByVal e As System.Windows.Forms.FormClosedEventArgs) Handles Me.FormClosed
        Gcls.ExitForm()
    End Sub

    Private Sub Customers_Load(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles MyBase.Load
        Try

            cls.RefreshData(TName)
            cls.RefreshData("Employees")
            cls.RefreshData("Stocks")

            MasterField1.TextBox1.MaxLength = 50

            Dim B As New BindingSource
            B.DataSource = MyDs
            B.DataMember = "Table_Columns"
            B.Filter = "Table_ID ='" & TName & "'"
            OrderByCombo.DataSource = B
            OrderByCombo.DisplayMember = "Logical_Name"
            OrderByCombo.ValueMember = "Physical_Name"

            '-------------------------------
            'Must Specify Data Table Name
            '-------------------------------

            Gcls = New GeneralSp.NewMasterForms(TName, BSourceCheckHeader, BtnNew, BtnSave, BtnDelete, BtnSearch, BtnExit, BtnReload, BtnCancelSerach, BtnFirst, BtnPrevious, BtnNext, BtnLast, BtnFile, BtnData, BtnHelp, OrderByCombo, ContentPanel, MasterField1, CountRecords)

            '-------------------------------
            'Must Specify Window Title 
            '-------------------------------

            Gcls.SetWTitle = "»Ì«‰«  «·Ã—œ"
            Me.Text = Gcls.SetWTitle

            MasterField1.TextBox1.DataBindings.Add("Text", BSourceCheckHeader, "Check_Name")
            CreatedDate.DataBindings.Add("Text", BSourceCheckHeader, "Created_Date")
            CreatedTime.DataBindings.Add("Text", BSourceCheckHeader, "Created_Time")

            EmployeeID.DataSource = MyDs
            EmployeeID.DisplayMember = "Employees.Employee_Name"
            EmployeeID.ValueMember = "Employees.Employee_ID"
            EmployeeID.DataBindings.Add("SelectedValue", BSourceCheckHeader, "Employee_ID")

            StockID.DataSource = MyDs
            StockID.DisplayMember = "Stocks.Stock_Name"
            StockID.ValueMember = "Stocks.Stock_ID"
            StockID.DataBindings.Add("SelectedValue", BSourceCheckHeader, "Stock_ID")

            SSource = BSourceCheckHeader

            '-------------------------------
            'Must Specify search field
            '-------------------------------

            RVal = "Check_Name"

            B_EndLoad = True

        Catch ex As Exception
            cls.WriteError(ex.Message, TName)
        End Try
    End Sub

    Private Sub BtnNew_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles BtnNew.Click
        Try
            MyDs.Tables("Check_Header").Columns("Created_Date").DefaultValue = Now.ToString("dd/MM/yyyy")
            MyDs.Tables("Check_Header").Columns("Created_Time").DefaultValue = Now.ToShortTimeString

            Gcls.NewRecord()
        Catch ex As Exception
            cls.WriteError(ex.Message, TName)
        End Try
    End Sub

    Private Sub BtnSave_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles BtnSave.Click
        Try
            If MasterField1.TextBox1.Text = "" Then
                cls.MsgComplete()
                MasterField1.TextBox1.Focus()
                MasterField1.TextBox1.BackColor = Color.Red
            ElseIf StockID.Text = "" Or EmployeeID.Text = "" Then
                cls.MsgComplete()
            Else
                Gcls.SaveRecord()
            End If
        Catch ex As Exception
            cls.WriteError(ex.Message, TName)
        End Try
    End Sub

